{
  "name": "cloudcmd-io",
  "version": "16.16.0",
  "private": true,
  "changelog": false,
  "release": false,
  "tag": false,
  "description": "io",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "redrun head update:index update:image",
    "update:image": "cd img/screen && redrun update:image:*",
    "update:index": "curl https://raw.githubusercontent.com/coderaiser/cloudcmd/master/HELP.md >> index.md",
    "update:image:view": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/view.png",
    "update:image:edit": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/edit.png",
    "update:image:console": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/console.png",
    "update:image:terminal": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/terminal.png",
    "update:image:menu": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/menu.png",
    "update:image:one-panel-mode": "wget -N https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/one-file-panel.png",
    "update:image:config": "wget -N -q https://raw.githubusercontent.com/coderaiser/cloudcmd/master/img/screen/config.png",
    "head": "cat head.md > index.md",
    "wisdom": "npm run release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudcmd/ru.git"
